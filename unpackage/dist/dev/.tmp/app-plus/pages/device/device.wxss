.device {
  padding: 100rpx 90rpx;
}
.device .login-logo {
    text-align: center;
    margin-bottom: 20rpx;
}
.device .login-logo image {
      width: 276rpx;
      height: 101rpx;
}
.device .device-txt {
    font-size: 30rpx;
    text-align: center;
    margin-bottom: 100rpx;
}
.device .content .list {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-justify-content: space-around;
        -ms-flex-pack: distribute;
            justify-content: space-around;
}
.device .content .list text {
      width: 70rpx;
      height: 70rpx;
      line-height: 70rpx;
      text-align: center;
      font-size: 40rpx;
      color: #66CC33;
      background: url(data:image/png;base64,) no-repeat;
      background-size: 100% 100%;
      border-radius: 50%;
}
.device .content .list text.cur {
        color: #fff;
        background: url(data:image/png;base64,) no-repeat;
}
.device .content .btn {
    width: 465rpx;
    height: 77rpx;
    line-height: 77rpx;
    text-align: center;
    background: #69bb19;
    font-size: 24rpx;
    color: #fff;
    margin: 120rpx auto 0;
    border-radius: 10rpx;
}

